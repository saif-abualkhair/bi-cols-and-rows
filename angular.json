{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-bootstrap-blocks": {
      "projectType": "library",
      "root": "projects/ngx-bootstrap-blocks",
      "sourceRoot": "projects/ngx-bootstrap-blocks/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-bootstrap-blocks/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-bootstrap-blocks/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-bootstrap-blocks/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-bootstrap-blocks/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "projects/ngx-bootstrap-blocks/.storybook",
            "browserTarget": "ngx-bootstrap-blocks:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/ngx-bootstrap-blocks"
            ],
            "styles": [
              {
                "input": "node_modules/bootstrap/scss/bootstrap.scss"
              }
            ],
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/ngx-bootstrap-blocks/.storybook",
            "browserTarget": "ngx-bootstrap-blocks:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/ngx-bootstrap-blocks"
            ],
            "outputDir": "dist/storybook/ngx-bootstrap-blocks"
          }
        }
      }
    },
    "bi-cols-and-rows-testing-project": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/bi-cols-and-rows-testing-project",
      "sourceRoot": "projects/bi-cols-and-rows-testing-project/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/bi-cols-and-rows-testing-project",
            "index": "projects/bi-cols-and-rows-testing-project/src/index.html",
            "browser": "projects/bi-cols-and-rows-testing-project/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/bi-cols-and-rows-testing-project/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/bi-cols-and-rows-testing-project/public"
              }
            ],
            "styles": [
              {
                "input": "node_modules/bootstrap/scss/bootstrap.scss"
              },
              "projects/bi-cols-and-rows-testing-project/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "bi-cols-and-rows-testing-project:build:production"
            },
            "development": {
              "buildTarget": "bi-cols-and-rows-testing-project:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/bi-cols-and-rows-testing-project/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/ngx-bootstrap-blocks-testing-project/public"
              }
            ],
            "styles": [
              "projects/ngx-bootstrap-blocks-testing-project/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}